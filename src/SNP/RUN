./sara_good.py ./PKX/PKX_500022_WT.sam ./PKX/PKX_500022_MUT.sam ./PKX/EA_PXK PKX_500022.txt >PKX_500022.log

./sara_good.py ./IRF5/IRF5_run0128_lane3_read1_index21=100344-9050_WT.sam ./IRF5/IRF5_run0128_lane3_read1_index21=100344-9050_MUT.sam ./IRF5/EA_IRF5_All IRF5_100344.txt>IRF5_100344.log
./sara_good.py ./IRF5/run0128_lane3_read1_index22=101679-9050_chr7_28543578..128771994_wt_s.sam ./IRF5/run0128_lane3_read1_index22=101679-9050_chr7_28543578..128771994_mut_s.sam ./IRF5/EA_IRF5_All IRF5_101679.txt>IRF5_101679.log
./sara_good.py ./IRF5/run0128_lane3_read1_index23=500022-9050_chr7_28543578..128771994_wt_s.sam ./IRF5/run0128_lane3_read1_index23=500022-9050_chr7_28543578..128771994_mut_s.sam ./IRF5/EA_IRF5_All IRF5_500022.txt>IRF5_500022.log

./sara_good.py ./STAT4/STAT4_100344_WT.txt ./STAT4/STAT4_100344_MUT.txt ./STAT4/EA_Stat4 STAT4_100344.txt > STAT4_100344.log
./sara_good.py ./STAT4/STAT4_101679_WT.txt ./STAT4/STAT4_101679_MUT.txt ./STAT4/EA_Stat4 STAT4_101679.txt > STAT4_101679.log
./sara_good.py ./STAT4/STAT4_500022_WT.txt ./STAT4/STAT4_500022_MUT.txt ./STAT4/EA_Stat4 STAT4_500022.txt > STAT4_500022.log
